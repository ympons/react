{
    "name": "react/react",
    "description": "ReactPHP: Event-driven, non-blocking I/O with PHP.",
    "keywords": ["reactor", "asynchronous", "ReactPHP"],
    "homepage": "https://reactphp.org/",
    "license": "MIT",
    "support": {
        "chat": "https://gitter.im/reactphp/reactphp"
    },
    "require": {
        "php": ">=5.3.8",
        "react/async": "^4.2 || ^3.2 || ^2.2",
        "react/cache": "^1.1",
        "react/dns": "^1.12",
        "react/event-loop": "^1.4",
        "react/http": "^1.10",
        "react/promise": "^3 || ^2.10 || ^1.3",
        "react/promise-stream": "^1.7",
        "react/promise-timer": "^1.10",
        "react/socket": "^1.14",
        "react/stream": "^1.3"
    },
    "require-dev": {
        "clue/stream-filter": "^1.3",
        "phpunit/phpunit": "^9.6 || ^7.5 || ^5.7 || ^4.8.36"
    },
    "config": {
        "preferred-install": {
            "react/*": "source"
        }
    }
}
